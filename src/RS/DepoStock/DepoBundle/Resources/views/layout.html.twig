<html>
<head>

    <title>DEPOSTOCK | {% block titulo %} {% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="{{ asset('css/bootstrap.css') }}" rel="stylesheet" media="screen">
    <link href="{{ asset('bundles/depo/css/jquery-ui.css') }}" rel="stylesheet" media="screen">
    <link href="{{ asset('bundles/depo/css/estilo.css') }}" rel="stylesheet" media="screen">
    
    <script src="{{ asset('js/jquery_jquery-1.11.1_1.js') }}"></script>
    <script src="{{ asset('bundles/depo/js/jquery-ui.js') }}"></script>
    
    
    
    {% block stylesheets %}{% endblock %}
        
    <!-- HTML5 Shim and Respond.js add IE8 support of HTML5 elements and media queries -->
    {% include 'BraincraftedBootstrapBundle::ie8-support.html.twig' %}

</head>

<body>
    	
    <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                        <a class="navbar-brand" href="#"><span>Depostock</span></a>
                    </div>
                    <div class="collapse navbar-collapse">
                        {% if is_granted('ROLE_EMPLEADO') %}
                            {{ knp_menu_render('DepoBundle:Builder:mainMenu') }}
                        {% endif %}
                    </div>
                
            </div>
        </div>    
                    <br /><br /><br /><br />
    {% block body %}{% endblock %}

    {% block javascripts %}{% endblock %}
    <!-- jQuery (necessary for Bootstraps JavaScript plugins) -->
    <script src="{{ asset('js/jquery.js') }}"></script>
    <!-- Include all JavaScripts, compiled by Assetic -->
    <script src="{{ asset('js/bootstrap.js') }}"></script>
</body>
</html>
